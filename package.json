{
  "name": "micro-frontend-todo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mock-data.json",
    "start:proxy": "ng serve --prod --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:proxy'",
    "start:create:proxy": "ng serve create-todo --port=4201 --proxy-config proxy.conf.json",
    "start:proxy:create:server": "npm run start:create:proxy",
    "start:proxy:mock:create:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:create:proxy'",
    "build:create-todo:elements": "ng build create-todo --prod --output-hashing none && node projects/create-todo/build/bundle.js",
    "start:view:proxy": "ng serve view-todo --port=4202 --proxy-config proxy.conf.json",
    "start:proxy:view:server": "npm run start:view:proxy",
    "start:proxy:mock:view:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:view:proxy'",
    "build:view-todo:elements": "ng build view-todo --prod --output-hashing none && node projects/view-todo/build/bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/elements": "^9.0.5",
    "@angular/forms": "~9.0.3",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "bootstrap": "^4.4.1",
    "document-register-element": "^1.7.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.20",
    "codelyzer": "^5.1.2",
    "concat": "^1.0.3",
    "concurrently": "^5.1.0",
    "del": "^5.1.0",
    "fs-extra": "^9.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "json-server": "^0.16.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.2",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
