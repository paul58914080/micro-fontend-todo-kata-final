{
  "name": "micro-frontend-todo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mock-data.json",
    "start:proxy": "ng serve --prod --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:proxy'",
    "start:create:proxy": "ng serve create-todo --port=4201 --proxy-config proxy.conf.json",
    "start:proxy:create:server": "npm run start:create:proxy",
    "start:proxy:mock:create:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:create:proxy'",
    "build:create-todo:elements": "ng build create-todo --prod --output-hashing none && node projects/create-todo/build/bundle.js",
    "start:view:proxy": "ng serve view-todo --port=4202 --proxy-config proxy.conf.json",
    "start:proxy:view:server": "npm run start:view:proxy",
    "start:proxy:mock:view:server": "concurrently --kill-others 'npm run mock:server' 'npm run start:view:proxy'",
    "build:view-todo:elements": "ng build view-todo --prod --output-hashing none && node projects/view-todo/build/bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/elements": "^8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "document-register-element": "^1.7.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.1.0",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concat": "^1.0.3",
    "concurrently": "^4.1.1",
    "del": "^5.0.0",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
